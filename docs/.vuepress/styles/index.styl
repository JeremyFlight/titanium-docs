.member-list
  & h2, h3, h4
    margin-top: -3.1rem;
    padding-top: 4.6rem;
    margin-bottom: 0;

  .member-header
    display flex

    &>h3, h4
      flex-grow 1

.proxy-meta
  margin-top 10px

  &>.proxy-meta-name
    font-size: 14px
    color: #666666
    text-align right

  &>.proxy-meta-value
    margin-top: 5px;
    text-align right

.custom-block
  &.tip
    border-color #19AEEF !important

.go-to-top
  color $accentColor !important

.api-page
  &>.page
    &>.content
      &>h1:first-child
        padding-top 0